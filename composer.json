    "blockify/framework": "@dev",
    "tgmpa/tgm-plugin-activation": "dev-develop"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../plugins/blockify",
      "options": {
        "symlink": false
      }
    }
  ]
}
