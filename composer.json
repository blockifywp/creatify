  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Blockify",
      "email": "info@blockify.com"
    }
  ],
  "type": "wordpress-theme",
  "minimum-stability": "dev",
  "scripts": {
    "install:framework": "rm -Rf vendor/blockify && rm -Rf composer.lock && composer dump && composer install"
